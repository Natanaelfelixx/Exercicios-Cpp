#include <algorithm>
#include <iostream>
using namespace std;

bool coprime(short a, short b){
    return (gcd(a,b) == 1);
}

int main(){
    int a, b;
    cout << "Entre com a e b: " << endl;
    cin >> a >> b;
    
    if(coprime(a, b) == true){
        cout << "Os numeros " << a << " e " << b << " sao coprimos" << endl;
    }else{
        cout << "Os numeros " << a << " e " << b << " nao sao coprimos" << endl;
    }
    
    return 0;
}
